{
  "name": "fiedsch/ligaverwaltung-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 Bundle: Ligaverwaltung",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Fieger",
      "email": "dev@andreas-fieger.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "contao/core-bundle": "^4.4",
    "eluceo/ical": "^0.13",
    "fiedsch/contao-jsonwidget": "^0.1.2",
    "codefog/contao-haste": "^4.24.1",
    "sensio/framework-extra-bundle": "^5.4.1"
  },
  "autoload": {
    "psr-4": {
      "Fiedsch\\LigaverwaltungBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/assets",
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/templates"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Fiedsch\\LigaverwaltungBundle\\ContaoManager\\Plugin",
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13"
  }
}
